{
    "name": "discord-manager",
    "version": "1.0.0",
    "main": "index.js",
    "license": "MIT",
    "dependencies": {
        "@types/node": "^20.10.6",
        "discord.js": "^14.14.1",
        "dotenv": "^16.3.1",
        "eslint": "^8.56.0",
        "eslint-import-resolver-typescript": "^3.6.1",
        "eslint-plugin-import": "^2.29.1",
        "mongoose": "^8.0.3",
        "nodemon": "^3.0.2",
        "pretty-ms": "^8.0.0",
        "tsconfig-paths": "^4.2.0"
    },
    "scripts": {
        "build": "tsc",
        "dev": "nodemon --exec ts-node-dev --inspect --transpile-only --ignore node_modules src/index.ts",
        "start": "nodemon --exec ts-node src/index.ts",
        "lint": "eslint . --ext .ts",
        "test": "jest",
        "test:watch": "jest --watch",
        "test:coverage": "jest --coverage"
    },
    "jest": {
        "preset": "ts-jest",
        "testEnvironment": "node",
        "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.[jt]sx?$",
        "moduleFileExtensions": [
            "ts",
            "tsx",
            "js",
            "jsx",
            "json",
            "node"
        ]
    },
    "devDependencies": {
        "@types/cors": "^2.8.17",
        "@types/express": "^4.17.21",
        "@types/jest": "^29.5.11",
        "@types/ms": "^0.7.34",
        "@types/pretty-ms": "^5.0.1",
        "@typescript-eslint/eslint-plugin": "^6.17.0",
        "@typescript-eslint/parser": "^6.17.0",
        "eslint-config-prettier": "^9.1.0",
        "eslint-plugin-prettier": "^5.1.2",
        "eslint-plugin-react": "^7.33.2",
        "jest": "^29.7.0",
        "prettier": "^3.1.1",
        "ts-jest": "^29.1.1",
        "ts-node": "^10.9.2",
        "ts-node-dev": "^2.0.0",
        "typescript": "^5.3.3"
    }
}